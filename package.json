{
  "name": "kraken-website-tentacle",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "test": "NODE_ENV=test mocha --reporter spec ./test/**/*.spec.js",
    "test-it": "NODE_ENV=test mocha --reporter spec ./test/**/*.spec.it.js",
    "db:create": "node ./config/tasks/createDb.js",
    "db:drop": "node ./config/tasks/dropDb.js",
    "db:insert:testData": "node ./config/tasks/insertTestData.js",
    "db:setup:dev": "npm run db:drop && npm run db:create && node ./config/tasks/insertTestData.js"
  },
  "dependencies": {
    "body-parser": "~1.14.2",
    "cookie-parser": "~1.4.0",
    "cron": "^1.1.0",
    "express": "~4.13.3",
    "express-validator": "^2.18.0",
    "glob": "^6.0.4",
    "hbs": "^4.0.0",
    "mime": "^1.3.4",
    "nconf": "^0.8.2",
    "node-uuid": "^1.4.7",
    "q": "^1.4.1",
    "request-promise": "^1.0.2",
    "rethinkdb": "^2.2.1",
    "util": "^0.10.3",
    "xmldom": "^0.1.19",
    "xpath": "0.0.9"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "really-need": "^1.9.0",
    "supertest": "^1.1.0"
  }
}
